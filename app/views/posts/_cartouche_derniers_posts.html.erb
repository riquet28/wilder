<div class="panel panel-primary">
  <div class="panel-heading">
      <h3 class="panel-title">Derniers Posts
        <span class="badge pull-right">3</span></h3>
  </div>
  <div class="panel-body">
    <div class="row">
      <div class="col-md-12">
        <ul>
          <% @lastpost.each do |post| %>
            <li><%= post.content %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>